<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
              targetNamespace="urn:skl:tp:vagval:v2" 
              xmlns:tns="urn:skl:tp:vagval:v2" 
              xmlns:vi="urn:skl:tp:vagvalsinfo:v2" 
              elementFormDefault="qualified" 
              attributeFormDefault="unqualified" 
              version="2.0">

	<xs:annotation>
		<xs:documentation>
			Tjänsteplattform
			Tjänste-schema för Vägval
    		</xs:documentation>
	</xs:annotation>

	<xs:import schemaLocation="../vagvalsInfo/vagvalsinfo-virtualiseringsinfo-2.0.xsd" namespace="urn:skl:tp:vagvalsinfo:v2" />	

	<xs:annotation>
		<xs:documentation>
			Meddelanden för operationer i VagvalsInterface
    		</xs:documentation>
	</xs:annotation>

	<xs:annotation>
		<xs:documentation>
			Operation visaVagval
		</xs:documentation>
	</xs:annotation>
	<xs:element name="visaVagvalRequest" >
	<xs:complexType>
		<xs:sequence>
				<xs:element name="senderId" type="xs:string"/>	
				<xs:element name="receiverId" type="xs:string" />
				<xs:element name="tidpunkt" type="xs:dateTime" />
				<xs:element name="tjanstegranssnitt" type="xs:NCName" />
				<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	</xs:element>
	
	<xs:element name="visaVagvalResponse" >
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="vi:virtualiseringsInfo" minOccurs="0" maxOccurs="unbounded" />
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<xs:annotation>
		<xs:documentation>
			Operation resetVagvalCache
		</xs:documentation>
	</xs:annotation>
	<xs:element name="resetVagvalCacheRequest" >
	<xs:complexType>
		<xs:sequence>
				<xs:any namespace="##other" processContents="lax" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	</xs:element>
	
	<xs:element name="resetVagvalCacheResponse" >
		<xs:complexType>
			<xs:sequence>
				<xs:element name="resetResult" type="xs:boolean" minOccurs="1"/>
				<xs:element name="processingLog" type="xs:string" minOccurs="1" maxOccurs="unbounded"/>  
			</xs:sequence>
		</xs:complexType>
	</xs:element>

</xs:schema>
