<beans xmlns="http://www.springframework.org/schema/beans"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:jaxws="http://cxf.apache.org/jaxws"
     xmlns:spring="http://www.springframework.org/schema/beans"
      xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
 						http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

    <!-- Load CXF modules from cxf.jar -->
    <import resource="classpath:META-INF/cxf/cxf.xml" />
    <import resource="classpath:META-INF/cxf/cxf-extension-soap.xml" />
    <import resource="classpath:META-INF/cxf/cxf-servlet.xml" />

	<bean id="sokVagvalsInfo" class="se.skl.tp.vagval.admin.services.SokVagvalsInfoImpl">
		<property name="vagvalSyncService" ref="vagvalSyncService"/>
	</bean>
	
	<bean id="getSupportedServiceContracts" class="se.skl.tp.vagval.admin.services.GetSupportedServiceContractsImpl">
		<property name="vagvalSyncService" ref="vagvalSyncService" />
	</bean>

	<bean id="getLogicalAddresseesByServiceContract" class="se.skl.tp.vagval.admin.services.GetLogicalAddresseesByServiceContractImpl">
		<property name="vagvalSyncService" ref="vagvalSyncService" />
	</bean>

  	<jaxws:endpoint id="sokvagval"
                  	implementor="#sokVagvalsInfo"
                  	address="/SokVagvalsInfo" />
                  	
	<jaxws:endpoint id="getsupportedservicecontracts" 
					implementor="#getSupportedServiceContracts"
					address="/GetSupportedServiceContracts" />

	<jaxws:endpoint id="getLogicaladdresseesbyservicecontract" 
					implementor="#getLogicalAddresseesByServiceContract"
					address="/GetLogicalAddresseesByServiceContract" />
                      	
</beans>